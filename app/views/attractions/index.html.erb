<% @attractions.each do |attraction|%>
  <%=attraction.name%><br />
  <%=attraction.tickets%><br />
<% if @user.admin? == true %>
  <%= link_to "Show #{attraction.name}", attraction_path(attraction) %><br />
  <% else %>
  <%= link_to "Go on #{attraction.name}",attraction_path(attraction) %><br /><br />
  <%end%>
<% end %> 
<% if @user.admin? == true %>
  <%= link_to "New Attraction", new_attraction_path %><br />
  <%end%>
